{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/notes.json", "methods": ["GET"], "dest": "/index.js" },
      { "src": "/api/add", "methods": ["GET"], "dest": "/index.js" },
      { "src": "/(.*)", "dest": "/index.js" }
    ]
  }
  